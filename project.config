[access]
	inheritFrom = All-Projects
[project]
	state = active
[access "refs/*"]
	owner = group ldap/gerrit-vieux-committers
	read = group Non-interactive vieux users
	read = group ldap/gerrit-vieux-committers
	forgeAuthor = group ldap/gerrit-vieux-committers
[access "refs/for/refs/*"]
	push = group ldap/gerrit-vieux-committers
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group ldap/gerrit-vieux-committers
	label-Code-Review = -1..+1 group Non-interactive vieux users
	label-Verified = -1..+1 group Non-interactive vieux users
	label-Verified = -1..+1 group ldap/gerrit-vieux-committers
	submit = group ldap/gerrit-vieux-committers
[access "refs/tags/*"]
	pushTag = group ldap/gerrit-vieux-committers
