[access]
	inheritFrom = All-Projects
[project]
	state = active
	description = OpenDaylight UX
[access "refs/*"]
	owner = group ldap/gerrit-dlux-committers
	read = group Non-interactive dlux users
	read = group ldap/gerrit-dlux-committers
	forgeAuthor = group ldap/gerrit-dlux-committers
[access "refs/for/refs/*"]
	push = group ldap/gerrit-dlux-committers
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group ldap/gerrit-dlux-committers
	label-Code-Review = -1..+1 group Non-interactive dlux users
	label-Verified = -1..+1 group Non-interactive dlux users
	label-Verified = -1..+1 group ldap/gerrit-dlux-committers
	submit = group ldap/gerrit-dlux-committers
	create = group Non-interactive dlux users
	forgeAuthor = group Non-interactive dlux users
	forgeCommitter = group Non-interactive dlux users
	push = group Non-interactive dlux users
	pushMerge = group Non-interactive dlux users
[access "refs/tags/*"]
	pushTag = group Non-interactive dlux users
	pushTag = +force group ldap/gerrit-dlux-committers
	create = group ldap/gerrit-dlux-committers
	forgeCommitter = group Non-interactive dlux users
	push = group Non-interactive dlux users
	push = +force group ldap/gerrit-dlux-committers
[receive]
	requireSignedOffBy = false
[submit]
	mergeContent = false
	action = merge always
