[access]
	inheritFrom = All-Projects
[project]
	description = OpenDaylight UX
[access "refs/*"]
	owner = group ldap/odl-gerrit-dlux-committers
	owner = group saml/odl-gerrit-dlux-committers
	read = group GitHub Replication
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[allowSuperproject "releng/autorelease"]
	matching = refs/heads/stable/nitrogen:refs/heads/stable/nitrogen
